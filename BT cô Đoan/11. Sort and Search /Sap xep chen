#include <bits/stdc++.h>
using namespace std;

#define el '\n'
#define ll long long
#define st first
#define nd second
#define pii pair<int,int>
#define psi pair<string,int>

const long long INF = 1e15;
const int nm = 2e5 + 9;
const int N = 1e3 + 9;
const int MOD = 1e9 + 7;
int a[nm];
void solve(){
	int n; cin >> n;
	for (int i = 1; i <= n; i++){
		cin >> a[i];
	}
	for (int i = 2; i <= n; i++){
		int j = i; 
		while (j > 0 && a[i] < a[j - 1]) j--; 
		int tmp = a[i]; 
		for (int k = i; k > j; k--)
			a[k] = a[k - 1]; 
		a[j] = tmp; 
		cout << "Buoc " << i - 1 << ": "; 
		for (int i = 1;i <= n; i++) cout << a[i] << " ";
		cout << el;  
	}
}

signed main (){
	int t; t = 1;
	while (t--){
		solve();
	}
	return 0;
}
