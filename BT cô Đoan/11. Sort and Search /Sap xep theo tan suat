#include <bits/stdc++.h>

using namespace std;
const int nm = 1e6 + 9; 
int a[nm], mp[nm], start[nm];
pair <int,int> b[nm]; 
bool cmp1(int a, int b){
	if (mp[a] == mp[b]) return a < b; 
	return mp[a] > mp[b]; 
}

bool cmp2(pair<int,int> a, pair<int,int> b){
	if (mp[a.first] == mp[b.first]) return a.second < b.second; 
	return mp[a.first] > mp[b.first]; 
}

int main (){
	int n; cin >> n;
	for (int i = 1; i <= n; i++){
		cin >> a[i]; b[i].first = a[i];
		if (mp[b[i].first] == 0){
			start[b[i].first] = i; 
			b[i].second = i; 
		}
		else b[i].second = start[b[i].first]; 
		mp[a[i]]++;
	}
	sort (a + 1, a + 1 + n, cmp1); 
	for (int i = 1; i <= n; i++) cout << a[i] << " "; 
	sort (b + 1, b + 1 + n, cmp2);
	cout << endl; for (int i = 1; i <= n; i++) cout << b[i].first << " ";
	return 0;
}
