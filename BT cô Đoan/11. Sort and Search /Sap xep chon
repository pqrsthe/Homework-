#include <bits/stdc++.h>
using namespace std;

#define el '\n'
#define ll long long
#define st first
#define nd second
#define pii pair<int,int>
#define psi pair<string,int>

const long long INF = 1e15;
const int nm = 2e5 + 9;
const int N = 1e3 + 9;
const int MOD = 1e9 + 7;
int a[nm];
void solve(){
	int n; cin >> n;
	for (int i= 1; i <= n; i++) cin >> a[i]; 
	for (int i = 1; i < n; i++){
		int mn = 2e9; 
		for (int j = i; j <= n; j++)
			mn = min(mn , a[j]);
		for (int j = i + 1; j <= n; j++)
			if (a[j] == mn){
				swap(a[i],a[j]); break; 
			}
		cout << "Buoc " << i <<": ";
		for (int i = 1; i <= n; i++) cout << a[i] << " "; 
		cout << el; 	
	}
}

signed main (){
	int t; t = 1;
	while (t--){
		solve();
	}
	return 0;
}
