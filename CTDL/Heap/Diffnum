#include <bits/stdc++.h>

using namespace std;

#define ll long long
#define el '\n'
#define st first
#define nd second

const int nm = 2e5 + 9;
const long long INF = 1e18;
const int MOD = 1e9 + 7;
const int N = 2e3 + 9;
priority_queue<int> pq; 
int a[nm]; 
void solve(){
	int n; cin >> n;
	for (int i = 1; i <= n; i++){
		cin >> a[i]; pq.push(a[i]); 
	}
	int dem = 0; 
	while (!pq.empty()){
		int mx = pq.top(); 
		if (mx == pq.top()){
			dem++; 
			while (!pq.empty() && mx == pq.top()){
				pq.pop(); 
			}
		}
	}
	cout << dem; 
}

signed main (){
	int t; t = 1;
	while (t--){
		solve();
	}
	return 0;
}
